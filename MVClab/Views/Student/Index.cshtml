<table class="table">
    <thead>
        <tr>
            <th scope="col">SSN</th>
            <th scope="col">Name</th>
            <th scope="col">Age</th>
            <th scope="col">Address</th>
        </tr>
    </thead>
    <tbody>
            @foreach(var item in Model)
            {
            <tr>
                <td>@item.SSN</td>
                <td>@item.Name</td>
                <td>@item.Age</td>
                <td>@item.Address</td>
            </tr>
            }
    </tbody>
</table>